{
  "name": "angular2-jspm-starter",
  "version": "1.0.0",
  "description": "Angular 2 starter build with JSPM, Gulp, TypeScript.",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "jspm install",
    "start": "./node_modules/gulp/bin/gulp.js start",
    "start:prod": "npm run build && ./node_modules/gulp/bin/gulp.js server",
    "test": "./node_modules/gulp/bin/gulp.js test",
    "test:watch": "./node_modules/gulp/bin/gulp.js test:watch",
    "build": "./node_modules/gulp/bin/gulp.js build",
    "gulp": "./node_modules/gulp/bin/gulp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samouss/angular2-jspm-starter.git"
  },
  "author": "Samuel Vaillant <samuel.vllnt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samouss/angular2-jspm-starter/issues"
  },
  "homepage": "https://github.com/samouss/angular2-jspm-starter#readme",
  "engines": {
    "node": ">=v4.0.0",
    "npm": ">=3.0.0"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.8",
    "babel-polyfill": "6.6.1",
    "browser-sync": "2.11.1",
    "chai": "3.5.0",
    "del": "2.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed": "1.3.0",
    "gulp-html-replace": "1.5.5",
    "gulp-hub": "0.7.1",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-replace": "0.5.4",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "4.3.3",
    "gulp-typescript": "2.12.1",
    "jspm": "0.16.30",
    "karma": "0.13.21",
    "karma-chai": "0.1.0",
    "karma-jspm": "2.0.2",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "1.2.3",
    "karma-phantomjs-launcher": "1.0.0",
    "lodash": "4.6.1",
    "merge-stream": "1.0.0",
    "mocha": "2.4.5",
    "phantomjs-prebuilt": "2.1.4",
    "require-dir": "0.3.0",
    "rxjs": "5.0.0-beta.2",
    "systemjs-builder": "0.15.10",
    "through": "2.3.8",
    "tslint": "3.5.0",
    "typescript": "1.8.7",
    "yargs": "4.2.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "configFile": "jspm.config.js",
    "dependencies": {
      "angular2": "npm:angular2@2.0.0-beta.8",
      "bootstrap": "github:twbs/bootstrap@3.3.6",
      "css": "github:systemjs/plugin-css@0.1.20",
      "rxjs": "npm:rxjs@5.0.0-beta.2"
    },
    "devDependencies": {
      "clean-css": "npm:clean-css@^3.4.9"
    },
    "overrides": {
      "npm:angular2@2.0.0-beta.1": {
        "shim": {
          "bundles/angular2-polyfills.min": {
            "format": "global"
          }
        }
      }
    }
  }
}
